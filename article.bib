@article{newman_lying_2003,
	title = {Lying {Words}: {Predicting} deception from linguistic styles},
	volume = {29},
	doi = {10.1177/0146167203251529},
	abstract = {Telling lies often requires creating a story about an experience or attitude that does not exist. As a result, false stories may be qualitatively different from true stories. The current project investigated the features of linguistic style that distinguish between true and false stories. In an analysis of five independent samples, a computer-based text analysis program correctly classified liar sand truth-tellers at a rate of 67\% when the topic was constant and a rate of 61\% overall. Compared to truth-tellers, liars showed lower cognitive complexity, used fewer self-references and other-references, and used more negative emotion words.},
