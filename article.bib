@article{bessi_subjective_2018,
	title = {From {Subjective} {Cognitive} {Decline} to {Alzheimer}’s {Disease}: {The} {Predictive} {Role} of\&nbsp;{Neuropsychological} {Assessment}, {Personality} {Traits}, and {Cognitive} {Reserve}. {A}\&nbsp;7-{Year} {Follow}-{Up} {Study}},
	volume = {Preprint},
	issn = {1387-2877},
	doi = {10.3233/jad-171180},
	abstract = {The aim of this study was to evaluate the accuracy of neuropsychological assessment in predicting conversion from subjective cognitive decline (SCD) and mild cognitive impairment (MCI) to Alzheimer’s disease (AD) and the effect of personality traits and cognitive reserve in progression from SCD to MCI. As part of a longitudinal, clinical-neuropsychological-genetic survey on SCD and MCI, 284 patients referred to our hospital between 1990 and 2017 were included. All patients underwent clinical-extensive neuropsychological evaluation and Apolipoprotein E genotyping; personality traits were assessed in a subgroup. Each patient underwent clinical-neuropsychological follow-up. Subjects with a follow-up shorter than two years were excluded. A total of 212 subjects were, after exclusions, considered: 26 out of 109 SCD subjects progressed to MCI (SCD-p), 15 converted to AD (SCD-c), and 68 remained stable (SCD-s). Of 103 MCI subjects, 39 converted to AD (MCI-c) and 64 remained stable (MCI-s). At baseline, SCD-c performed significantly worse than SCD-s in tests assessing long-term verbal memory. MCI-c showed worse performance on neuropsychological tests for short- and long-term verbal memory and for ecological evaluation of memory (RBMT). These tests provided good accuracy in distinguishing MCI-c and MCI-s. Emotional stability was significantly lower in SCD-s than in SCD-p while higher intellectual activities were associated with a lower risk of conversion to MCI. Our results suggest that memory neuropsychological tests may represent a reliable tool to estimate the risk of progression to AD. Personality and lifestyle factors could provide useful information to identify SCD subjects who may develop an objective cognitive impairment.},
	journal = {Journal of Alzheimer's Disease},
	author = {Bessi, Valentina and Mazzeo, Salvatore and Padiglioni, Sonia and Piccini, Carolina and Nacmias, Benedetta and Sorbi, Sandro and Bracco, Laura},
	year = {2018},
	pages = {1--13},
}

@article{furr_examining_2018,
	title = {{EXAMINING} {PERSONALITY} {AND} {DEMENTIA} {WORRY} {AMONG} {MIDDLE}-{AGED} {AND} {OLDER} {ADULTS}},
	volume = {2},
	doi = {10.1093/geroni/igy023.409},
	abstract = {Potential diagnosis of Alzheimer’s disease and related disorders (ADRD) can be a common concern for middle-aged and older adults. Kessler, Bowen, Baer, Froelich, and Wahl (2012) termed this specific type of anxiety “dementia worry (DW),” which involves distressing concern about developing dementia, even in the absence of cognitive impairment. DW can thus develop in individuals of all ages and cognitive status. This study examined whether personality traits are associated with DW. Participants were recruited from two online sources and consisted of 127 middle-aged and older adults (aged 40 – 81 years, M = 52.54, SD = 9.33). Participants completed a 20-item assessment of Big Five Personality traits (Donnellan, Oswald, Baird, \& Lucas, 2006) and the 12-item Dementia Worry Scale (Kinzer \& Suhr, 2016). Correlation analyses revealed that age was not associated with DW, p = .14. DW was negatively correlated with conscientiousness (r = -.21, p =.02), positively correlated with neuroticism (r = .32, p {\textless} .01), and unrelated to extroversion, agreeableness, and openness (ps {\textgreater} .07). Older age was associated with lower levels of neuroticism (r = -.18, p = .04) and unrelated to other personality variables. These findings expand our understanding of how personality relates to levels of concern middle-aged and older adults report about dementia. Based on the study design, causal order cannot be determined; however, given the likelihood that personality develops prior to concerns about dementia, results suggest that individual differences in personality are likely to influence level of DW.},
	journal = {Innovation in Aging},
	author = {Furr, E},
	year = {2018},
	pages = {110--110},
}

