}

@article{helgeson_characterizing_2013,
	title = {Characterizing the transition from paediatric to adult care among emerging adults with {Type} 1 diabetes},
	volume = {30},
	issn = {1464-5491},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC3628931/pdf/nihms-446418.pdf},
	doi = {10.1111/dme.12067},
	abstract = {The goals of the study were to describe the transition of youth with Type 1 diabetes from paediatric to adult healthcare services, examine the link of this transition with self care and glycaemic control, and distinguish youth who received medical treatment from different physicians in terms of demographic and parent relationship variables. Youth with Type 1 diabetes (n = 118) were enrolled in a prospective study that examined the transition from the paediatric to adult healthcare systems and were evaluated during their senior year of high school (time 1) and 1 year later (time 2). Data on self care, glycaemic control and parent relationship were collected. The majority of youth saw a paediatric endocrinologist at both assessments (n = 64); others saw an adult care physician at both assessments (n = 26) or transitioned from a paediatric endocrinologist to an adult care physician (n = 19). Nine youth saw no physician between time 1 and time 2. There were group differences in demographic and parent relationship variables and self-care behaviour and glycaemic control related to the transition of care. Youth who remained in the paediatric healthcare system had the best self care and did not experience declines in glycaemic control over time. Early transition from the paediatric healthcare system to the adult healthcare system is associated with psychosocial variables and worse glycaemic control. Future research should identify factors that determine optimal timing and strategies to avoid deterioration of care and control during this transition.},
	language = {English},
	journal = {Diabetic Medicine},
	author = {Helgeson, V. S. and Reynolds, K. A. and Snyder, P. R. and Palladino, D. K. and Becker, D. J. and Siminerio, L. and Escobar, O.},
	year = {2013},
	pages = {610--615},
}

