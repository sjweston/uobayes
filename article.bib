	doi = {10.1016/j.jagp.2013.03.007},
	abstract = {ObjectivesThe present study examines whether subjective age (i.e., how old or young individuals feel) is associated with cognitive functioning and tests potential mediators of this association.DesignData from the two waves of measurement of the Midlife in the United States survey were used, with assessments conducted at the first wave in 1994–1995 and at the second wave in 2004–2006.ParticipantsA total of 1,352 men and women aged from 50 to 75 years at baseline (M: 59.32; SD: 6.72).MeasurementsSubjective age, body mass index (BMI), physical activity, and the covariates sex, age, education, marital status, and disease burden were assessed at baseline to predict episodic memory and executive function measured 10 years later.ResultsMultiple regression analysis revealed that a younger subjective age at baseline was prospectively associated with better episodic memory and executive function. Bootstrap analysis indicated that the association of subjective age with episodic memory and executive function was partially mediated by BMI and frequency of physical activity respectively.ConclusionEven after accounting for chronological age and other risk factors for cognitive decline, such as disease burden and sedentary lifestyle, the subjective experience of aging predicts cognitive functioning in old age.},
	journal = {The American Journal of Geriatric Psychiatry},
	author = {Stephan, Yannick and Caudroit, Johan and Jaconelli, Alban and Terracciano, Antonio},
	year = {2014},
	pages = {1180--1187},
}

