	note = {Publisher: Elsevier},
	keywords = {pm10, mci},
	pages = {1004--1011},
	file = {Available Version (via Google Scholar):/Users/sweston2/Zotero/storage/R2WZZ2GY/Ranft et al. - 2009 - Long-term exposure to traffic-related particulate matter impairs cognitive function in the elderly.pdf:application/pdf},
