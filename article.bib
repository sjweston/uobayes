@article{graham_personality_2017,
	title = {Personality predicts mortality risk: {An} integrative data analysis of 15 international longitudinal studies},
	volume = {70},
	issn = {0092-6566},
	doi = {10.1016/j.jrp.2017.07.005},
	abstract = {This study examined the Big Five personality traits as predictors of mortality risk, and smoking as a mediator of that association. Replication was built into the fabric of our design: we used a Coordinated Analysis with 15 international datasets, representing 44,094 participants. We found that high neuroticism and low conscientiousness, extraversion, and agreeableness were consistent predictors of mortality across studies. Smoking had a small mediating effect for neuroticism. Country and baseline age explained variation in effects: studies with older baseline age showed a pattern of protective effects (HR{\textless}1.00) for openness, and U.S. studies showed a pattern of protective effects for extraversion. This study demonstrated coordinated analysis as a powerful approach to enhance replicability and reproducibility, especially for aging-related longitudinal research.},
	language = {English},
	journal = {Journal of Research in Personality},
	author = {Graham, Eileen K and Rutsohn, Joshua P and Turiano, Nicholas Andrew and Bendayan, Rebecca and Batterham, Philip J and Gerstorf, Denis and Katz, Mindy J and Reynolds, Chandra A and Sharp, Emily S and Yoneda, Tomiko B and Bastarache, Emily D and Elleman, Lorien G and Zelinski, Elizabeth M and Johansson, Boo and Kuh, Diana and Barnes, Lisa L and Bennett, David A and Deeg, Dorly J H and Lipton, Richard B and Pedersen, Nancy L and Piccinin, Andrea M and III, Avron Spiro and Muniz-Terrera, Graciela and Willis, Sherry L and Schaie, K Warner and Roan, Carol and Herd, Pamela and Hofer, Scott M and Mroczek, Daniel K},
	year = {2017},
