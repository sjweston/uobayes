@article{helgeson_brief_2010,
	title = {Brief {Report}: {Trajectories} of {Glycemic} {Control} over {Early} to {Middle} {Adolescence}},
	volume = {35},
	issn = {0146-8693},
	url = {https://watermark.silverchair.com/jsq011.pdf?token=AQECAHi208BE49Ooan9kkhW_Ercy7Dm3ZL_9Cf3qfKAc485ysgAAAmMwggJfBgkqhkiG9w0BBwagggJQMIICTAIBADCCAkUGCSqGSIb3DQEHATAeBglghkgBZQMEAS4wEQQMiMG8LPMkm1dMzhlxAgEQgIICFnpjMdmlDydq1BTNtR10-_8YYbfhKcSvcBVywotbP5pykvXo04q_unWacgQtW8e_133qrfvaRTW5jomP7ZoAsWAtqnRWReRGxpV_X7dXhzatmNxUkD_K2lxg_U4AdtN_3_EB3cw3xh_QBI7k8INcnyfCWVNwrldCYFsNk4gwQmSqhxk_M9WGGAz3Pcp798j7nHTE_T_Xw4Z6yFmXN33sEFP8aD-s119Tzic3AwJMnvz7Jw-hmuEaK-v-Q2UsD5HzVoosFfNL_2Z6cplywLLgHOy7YhhLz7180JQRiamWq5RsrJQpft4o12LgrgpvFYnSvIPKL7Yai8HSslN_Hv9PMlTrKiX3jkD9VsrFs38oClL6P9INMoLT_3pAMCDAXLOjmhphvhvSJ-1kd9VrZ167UeKCGhiIFe04VwPwd5OS_JNZpiLPcTIRsdEzNb2GMIGFSz5O3vtPVcZDFtauTN091EpBYCFQFF5z7z6Lp3ITree0HNcLFExa84FP-B4IxQ_oWY2epoD-IPN4JQQ8658fucaUqSDuEBZCbo0FQXJUk2_HvNF19Du945Z3LNnBlDFkjeJPyKTpOfgHe76sIR5azdfy2Ldi7bbZ-lOknG1TXWmiaRnOk45FkGOrLZS3Y7eXgCm0HQScsL-SMJwHioXD5uYqT0FZjaexOVdXL8o-h3TMbJ17es6gaahxH9af7dO4DFxIkwFtcQ},
	doi = {10.1093/jpepsy/jsq011},
	abstract = {Objectives To identify distinct patterns of glycemic control over early to middle adolescence, and to determine whether psychosocial variables predicted those patterns. Methods We used trajectory analysis to examine glycemic control over 5 years among adolescents with type 1 diabetes who were of age 12 on average at study start (n = 132). Well-being, relationships, and self-care behavior were assessed with in-person interviews. Blood glucose testing was determined from blood glucose meters, and missed clinic appointments and glycosolated hemoglobin were obtained from medical records. Results We identified two distinct clusters of individuals, a stable good glycemic control group and a poorer deteriorating glycemic control group. Individuals in the deteriorating control group were characterized by higher peer conflict, more negative diabetes emotions, fewer blood glucose tests, and more missed clinic appointments. Conclusion Psychosocial variables and behavioral markers of self-care may predict the course of glycemic control over early to middle adolescence.},
	language = {English},
	journal = {Journal of Pediatric Psychology},
	author = {Helgeson, Vicki S. and Snyder, Pamela R. and Seltman, Howard and Escobar, Oscar and Becker, Dorothy and Siminerio, Linda},
	year = {2010},
	pages = {1161--1167},
}

