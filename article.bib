@incollection{schulenberg_historical_2012,
	address = {New York, NY, US},
	series = {Duke series in child development and public policy},
	title = {Historical and developmental patterns of alcohol and drug use among college students: {Framing} the problem},
	isbn = {978-1-60623-995-7},
	shorttitle = {Historical and developmental patterns of alcohol and drug use among college students},
	abstract = {How do we best frame the problem of college student alcohol and drug use? Is it a problem that has been getting worse or better in recent years? Have the historical changes been specific to college students or more generally true of all in the 18- to 22-year-old age group? Indeed, is it more productive to view age 18-22 substance use as a college-specific issue or more of a time-of-life issue? What are the developmental trajectories of alcohol and drug use across the college years? How do these trajectories vary for those who drop out of, or never enroll in, college? And do the trajectories matter in terms of psychosocial adjustment? In this chapter, we consider these "big picture" questions, providing empirical evidence based on national multicohort panel data from the Monitoring the Future (MTF) study to frame college student substance use both historically and developmentally. We begin by providing an overview of historical variations, considering trends in several indices of substance use across three decades (1980-2009) as a function of college student status among national samples of young people 1 to 4 years post-high school. Then, after considering the developmental context that surrounds college transitions, we examine developmental trends in the two most common substances of abuse—alcohol and marijuana—from the senior year of high school through age 25 as a function of college status and of dropping out of college. Next, we examine different developmental trajectories of frequent heavy drinking (i.e., binge drinking), with particular attention given to college students who were not binge drinkers in high school. In the final section, we discuss theoretical and practical implications. (PsycInfo Database Record (c) 2022 APA, all rights reserved)},
	booktitle = {College drinking and drug use},
	publisher = {The Guilford Press},
	author = {Schulenberg, John E. and Patrick, Megan E.},
