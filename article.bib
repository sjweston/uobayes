	volume = {20},
	issn = {1088-8683},
	shorttitle = {The {Correlates} of {Similarity} {Estimates} {Are} {Often} {Misleadingly} {Positive}},
	url = {https://doi.org/10.1177/1088868315581119},
	doi = {10.1177/1088868315581119},
	abstract = {Research on similarity constructs (e.g., dyadic similarity, personality stability, judgment agreement and accuracy) frequently find them to be associated with positive outcomes. However, a methodological pitfall associated with common ?overall similarity? indices, which we term the normative-desirability confound (NDC), will regularly result in similarity constructs apparently having more positive effects than they do in reality. In essence, when an individual is estimated to be similar to another person by common indices, this will strongly indicate that the individual has desirable characteristics. Consequently, the correlates of overall similarity indices can often be interpreted as indicating the beneficial effects of having desirable characteristics, without needing to attribute any additional salutary effect to similarity. We show that this confound is present in overall similarity estimates for a wide range of constructs (e.g., personality traits, attitudes, emotions, behaviors, values), how it can be accounted for, and discuss larger implications for our understanding of similarity constructs.},
	number = {2},
	urldate = {2023-04-12},
	journal = {Personality and Social Psychology Review},
	author = {Wood, Dustin and Furr, R. Michael},
	month = may,
	year = {2016},
	note = {Publisher: SAGE Publications Inc},
	pages = {79--99},
	file = {The Correlates of Similarity Estimates Are Often M.pdf:/Users/sweston2/Zotero/storage/3V2QGXZV/The Correlates of Similarity Estimates Are Often M.pdf:application/pdf},
}

